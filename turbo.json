{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".next/**",
        "!./packages/components/@deprecated/*",
        "!./packages/tools/@deprecated/*"
      ]
    },
    "dev": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "package": {
      "dependsOn": ["build"],
      "inputs": [
        "./packages/*",
        "!./packages/components/@deprecated/*",
        "!./packages/tools/@deprecated/*"
      ],
      "outputs": []
    }
  }
}
