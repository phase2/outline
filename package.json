{
  "name": "@phase2tech/outline",
  "description": "Outline Component Design",
  "main": "dist/outline.js",
  "module": "dist/outline.js",
  "es2015": "dist/outline-legacy.js",
  "es2017": "dist/outline.js",
  "unpkg": "dist/outline.js",
  "files": [
    "dist/",
    ".prettierrc.json",
    ".eslintrc.json",
    "tailwind.config.js",
    "outline.theme.css"
  ],
  "author": {
    "name": "Phase2 Technology",
    "url": "https://phase2technology.com"
  },
  "contributors": [
    {
      "name": "Jake Strawn",
      "email": "jstrawn@phase2technology.com"
    }
  ],
  "license": "BSD-3-Clause",
  "scripts": {
    "preinstall": "npx check-node-version --node $(cat .nvmrc)",
    "prestart": "yarn prep",
    "start": "yarn storybook.dev.watch",
    "prep": "yarn generate.safelist && yarn postcss.storybook && yarn generate.resolved-tailwind && yarn generate.custom-elements && yarn generate.component-css",
    "generate.resolved-tailwind": "node scripts/resolve-tailwind-config.js",
    "generate.safelist": "node scripts/generate-tw-safelist.js",
    "generate.custom-elements": "wca analyze 'src/components/**/*[!(stories|lit)].ts' --format json --outFile ./src/custom-elements.json",
    "generate.component-css": "node scripts/lit-styles.js",
    "prebuild": "yarn prep",
    "build": "tsc && yarn rollup",
    "wds": "wds --app-index dev/index.html --watch --node-resolve",
    "preserve": "yarn prep",
    "serve": "concurrently -c \"cyan,blue\" \"yarn:rollup.watch\" \"yarn:wds\"",
    "storybook.wait": "wait-on ./src/.storybook/storybook.generated.css ./src/custom-elements.json ./src/resolved-tailwind-config.ts",
    "storybook.dev": "start-storybook -c src/.storybook -s ./src/.storybook/static -p 6006",
    "storybook.dev.css": "yarn postcss.storybook --watch",
    "storybook.dev.watch": "concurrently -n \"global css,component css,storybook\" -c \"yellow,green,blue\" \"yarn:storybook.dev.css\" \"yarn:generate.component-css -w\" \"yarn:storybook.dev\"",
    "storybook.prod": "yarn prep && yarn storybook.wait && NODE_ENV=production build-storybook -c src/.storybook -o storybook-static",
    "storybook.prod.build": "concurrently -n \"prep,rollup,storybook\" -c \"cyan,blue,green\" \"yarn:prep\" \"yarn:rollup\" \"yarn:storybook.prod\"",
    "storybook.prod.serve": "wds --app-index storybook-static/index.html --node-resolve",
    "lint": "yarn lint.eslint && yarn lint.prettier",
    "lint.eslint": "eslint .",
    "lint.prettier": "prettier --check .",
    "fmt": "concurrently \"yarn:fmt.code\" \"yarn:fmt.svg\"",
    "fmt.code": "eslint --fix . && prettier --write .",
    "fmt.svg": "svgo -f ./src -r --config=.svgo.yml",
    "semantic-release": "semantic-release",
    "ci": "rm -rf node_modules && yarn install --frozen-lockfile",
    "clean": "rimraf dist && rimraf src/custom-elements.json && rimraf src/resolved-tailwind-config.ts && rimraf storybook-static && rimraf src/**/*.lit.ts",
    "postcss.storybook": "TAILWIND_MODE=build postcss ./src/.storybook/storybook.css -o ./src/.storybook/storybook.generated.css --verbose",
    "reset": "rimraf node_modules",
    "reboot": "yarn clean && yarn reset && yarn install && yarn start",
    "rollup.bundle": "rollup -c -m",
    "rollup.watch": "yarn rollup -w",
    "rollup": "yarn rollup.bundle"
  },
  "keywords": [
    "web-components",
    "typescript",
    "lit"
  ],
  "dependencies": {
    "lit": "^2.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@storybook/addon-a11y": "^6.3.4",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^6.3.4",
    "@storybook/builder-webpack5": "^6.3.4",
    "@storybook/manager-webpack5": "^6.3.4",
    "@storybook/web-components": "^6.3.4",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "@web/dev-server": "0.0.29",
    "@web/dev-server-legacy": "^0.1.4",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.2",
    "deepmerge": "^4.2.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.0",
    "gaze": "^1.1.3",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "postcss-custom-properties": "^11.0.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.46.0",
    "rollup-plugin-summary": "^1.3.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "storybook": "^6.3.4",
    "svgo": "^1.3.2",
    "tailwindcss": "^2.1.0",
    "ts-lit-plugin": "^1.2.1",
    "typescript": "^4.0.0",
    "wait-on": "^5.3.0",
    "web-component-analyzer": "^1.0.3",
    "webpack": "^5.38.1"
  }
}
