{
  "name": "@phase2tech/outline-cli",
  "version": "0.0.0",
  "description": "Outline CLI",
  "author": "Phase2 Technology",
  "bin": {
    "outline": "./bin/run"
  },
  "homepage": "https://github.com/@phase2tech/outline",
  "license": "BSD-3-Clause",
  "main": "dist/index.js",
  "repository": "@phase2tech/outline",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "axios": "^0.25.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/inquirer": "^8.1.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/tar": "^6.1.1",
    "@types/validate-npm-package-name": "^3.0.3",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "inquirer": "^8.2.0",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.3",
    "tar": "^6.1.11",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "validate-npm-package-name": "^3.0.0"
  },
  "oclif": {
    "bin": "outline",
    "dirname": "oex",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/@phase2tech/outline/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
